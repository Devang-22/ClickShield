{
    "manifest_version": 3,
    "name": "Link Scanner",
    "version": "1.0",
    "description": "Scans the webpage for active links and clickable elements.",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  